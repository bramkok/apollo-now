{
  "version": 2,
  "alias": "tweakers.adsl.world",
  "builds": [{ "src": "api/index.js", "use": "@now/node" }],
  "routes": [{ "src": "/api/.*", "dest": "api/index.js" }],
  "regions": ["bru"],
  "env": {
    "MYSQL_HOST": "@mysql_host",
    "MYSQL_USER": "@mysql_user",
    "MYSQL_PASSWORD": "@mysql_password",
    "MYSQL_DATABASE": "@mysql_database"
  }
}
